import java.util.Scanner;


public class DockCMSC335 extends ThingCMSC335 {
    
   //Instance of ship in dock class
	ShipCMSC335 ship;
     //scan constructor for dock class   
	public DockCMSC335(Scanner sc) {
		super(sc);
	}
	//toString() method for object ---> string for dock
	public String toString() {
        //concatenated string dock to string method
        String dockString = "Dock: " + super.toString();
        //if statement if null return string dock
        if(ship == null) {
            //return the string
            return dockString;
        }
        // string concatenated with instance of ship
        dockString += "\n  " + ship;
        //retun string
        return dockString;
    } // end 
} // end 
